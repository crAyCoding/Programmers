#include <iostream>
#include <string>
#include <vector>
using namespace std;

int solution(string s)
{
    int answer = 1;
    vector<int> p;
    for(int i=0;i<s.size();i++)
    {
        if(p.empty() || p.back()!=s[i]) p.push_back(s[i]);
        else p.pop_back();
    }
    if(!p.empty()) answer = 0;
    
    // [실행] 버튼을 누르면 출력 값을 볼 수 있습니다.

    return answer;
}

//LV2
